{
  "functions": {
    "api/app.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/app.py"
    },
    {
      "source": "/download",
      "destination": "/api/app.py"
    },
    {
      "source": "/health",
      "destination": "/api/app.py"
    },
    {
      "source": "/status/(.*)",
      "destination": "/api/app.py"
    },
    {
      "source": "/file/(.*)",
      "destination": "/api/app.py"
    },
    {
      "source": "/privacy-policy",
      "destination": "/api/app.py"
    },
    {
      "source": "/terms",
      "destination": "/api/app.py"
    },
    {
      "source": "/about",
      "destination": "/api/app.py"
    },
    {
      "source": "/",
      "destination": "/api/app.py"
    }
  ]
}
